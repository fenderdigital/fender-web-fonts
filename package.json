{
  "name": "fender-web-fonts",
  "version": "1.0.0",
  "description": "Fender Web Fonts",
  "main": "index.js",
  "scripts": {
  "start": "npm run build:css && npm run build:minify && npm run copy",
  "build:css": "tachyons src/fonts.css > dist/fonts.css",
  "build:minify": "tachyons src/fonts.css -m > dist/fonts.min.css",
  "copy": "copyfiles -u 1 src/fonts/* dist"
},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fenderdigital/fender-web-fonts.git"
  },
  "author": "Fender Digital",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/fenderdigital/fender-web-fonts/issues"
  },
  "homepage": "https://github.com/fenderdigital/fender-web-fonts#readme",
  "devDependencies": {
    "copyfiles": "^1.2.0",
    "tachyons-cli": "^1.0.10"
  }
}
